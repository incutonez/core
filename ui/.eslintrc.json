{
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-strongly-recommended"
  ],
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "plugins": ["eslint-plugin-tailwindcss"],
  "rules": {
    "vue/attributes-order": "error",
    "tailwindcss/classnames-order": "error"
  }
}
